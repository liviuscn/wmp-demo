page{
  height: 100%;
  width: 100%;
}

.container{
    height: 100%;
    width: 100%;
    display: flex;
}

.nav{
    height: 100%;
    flex: 0 0 33.3333%;
    background-color: #999;
}

.content{
  height: 100%;
  flex: 0 0 66.6667%;
  display: flex;
}

.list{
  display: flex;
  flex-flow: row wrap;
  flex: 1;
  box-sizing: border-box;
  border:solid 20rpx #fff;
  height: 100%;
  align-content: flex-start;
  justify-content: flex-start;
  align-items: flex-start;
}

.item-wrap{
  display: flex;
  flex: 0 0 50%;
  box-sizing: border-box;
  border:solid 20rpx #fff;
}

.item{
  flex: 1;
  height: 100%;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.item image{
  height: 100px;
  width: 100%;
}